  <body>
    <div class="form-register">
      <form action="/register" method="post">
        <label for="">nama</label>
        <input type="nama" name="nama" placeholder="nama" autocomplete="off" />
        <label for="">email</label>
        <input
          type="email"
          name="email"
          placeholder="email"
          autocomplete="off"
        />
        <label for="">password</label>
        <input
          type="passowrd"
          name="passowrd"
          placeholder="passowrd"
          autocomplete="off"
        />
        <button type="submit">Submit</button>
      </form>
    </div>
  </body>
</html>
